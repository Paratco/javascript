{
  "name": "@paratco/eslint-config",
  "version": "3.2.0",
  "type": "module",
  "author": "Paratco",
  "license": "MIT",
  "description": "Paratco ESLint Config",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Paratco/javascript.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "react",
    "node",
    "node.js",
    "lint"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "scripts": {
    "lint": "oxlint --max-warnings=0 && eslint --max-warnings=0",
    "lint:fix": "oxlint --fix && eslint --fix",
    "lint:build": "migrate",
    "build:check": "tsc --noEmit",
    "build": "tsdown",
    "release": "semantic-release",
    "release-dry-debug": "semantic-release --no-ci -d --debug",
    "release-dry": "semantic-release --no-ci -d"
  },
  "dependencies": {
    "@eslint/js": "9.34.0",
    "@stylistic/eslint-plugin": "5.2.3",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-unicorn": "60.0.0",
    "eslint-plugin-unused-imports": "4.2.0",
    "globals": "16.3.0",
    "typescript-eslint": "8.41.0"
  },
  "devDependencies": {
    "@oxlint/migrate": "1.14.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.5",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/node": "^24.3.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint-plugin-oxlint": "1.14.0",
    "jiti": "^2.5.1",
    "oxlint": "1.14.0",
    "semantic-release": "^24.2.7",
    "tsdown": "^0.14.2",
    "typescript": "~5.9.2"
  },
  "packageManager": "yarn@4.9.4"
}
